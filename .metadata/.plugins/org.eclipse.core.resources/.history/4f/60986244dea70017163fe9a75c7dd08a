//*****************************************************************************
//
// MSP432 main.c template - Empty main
//
//****************************************************************************

#include "msp.h"
#include "Keypad.h"

int main(void) {
    unsigned char key;

    keypad_init();
    LED_init();

    while(1) {
        key = keypad_getkey();  /* read the keypad */
        LED_set(key);           /* set LEDs according to the key code */
    }
}
