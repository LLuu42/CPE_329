/*
 * main.c
 * Author: Lara Luu
 * Created on: Oct 2, 2017
 * Maps a 3x4 keyboard to a LED on the MSP432
 */


#include "msp.h"
#include "Keypad.h"

int main(void) {
    unsigned char key;

    keypad_init();
    LED_init();

    while(1) {
        key = keypad_getkey();  /* read the keypad */

        switch(key):
            case '0x01':
                break;
            case '0x02':
                break;
            case '0x03':
                break;
            case '0x04':
                break;
            case '0x05':
                break;
            case '0x06':
                break;
            case '0x07':
                break;
            case '0x08':
                break;
            case '0x09':
                break;
            case '0x10':
                break;
            case '0x11':
                break;
            case '0x12':
                break;

        LED_set(key);           /* set LEDs according to the key code */
    }
}
